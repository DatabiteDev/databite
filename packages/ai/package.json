{
  "name": "@databite/ai",
  "version": "1.1.1",
  "description": "AI-powered connector generator for Databite",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs"
    }
  },
  "bin": {
    "databite-ai": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:flow": "ts-node examples/flow-blocks-test.ts",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "cli": "node ./dist/cli.js"
  },
  "dependencies": {
    "@databite/types": "workspace:*",
    "@databite/build": "workspace:*",
    "ai": "^4.0.0",
    "@ai-sdk/openai": "^1.0.0",
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "commander": "^12.0.0",
    "cheerio": "^1.0.0",
    "ora": "^8.0.0",
    "chalk": "^5.3.0",
    "zod": "^3.22.0",
    "puppeteer": "^22.0.0",
    "turndown": "^7.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/turndown": "^5.0.0",
    "typescript": "^5.3.0",
    "tsup": "^8.3.5",
    "rimraf": "^6.0.1",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
