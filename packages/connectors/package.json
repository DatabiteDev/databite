{
  "name": "@databite/connectors",
  "version": "2.1.0",
  "description": "A library of pre-built connectors built with the Databite SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:flow": "ts-node examples/flow-blocks-test.ts",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "connector",
    "sdk",
    "integration",
    "api",
    "scheduler",
    "flow",
    "typescript"
  ],
  "author": "Databite",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DatabiteDev/databite.git"
  },
  "bugs": {
    "url": "https://github.com/DatabiteDev/databite/issues"
  },
  "homepage": "https://github.com/DatabiteDev/databite#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@databite/types": "workspace:*",
    "@databite/build": "workspace:*",
    "zod": "^4.1.11"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
